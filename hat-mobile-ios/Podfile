    pod 'SwiftyRSA'
    pod 'KeychainSwift'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'BEMCheckBox'
    pod 'zxcvbn-ios'
    pod 'HatForIOS', :git => 'https://github.com/whiteshadow-gr/HatForIOS.git'
    pod 'Mockingjay'
    pod 'MarkdownView'
    pod 'Haneke'
end

